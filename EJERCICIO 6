#include <stdio.h>
int main() 
{
  int i;
  float suma = 0.0;
    // Ciclo for para calcular la suma de los 10 primeros elementos
  for (i = 1; i <= 10; i++) {
    float zn = (3.0 * i) / (5.0 * i);
    suma += zn;
  }
  printf("La suma de los primeros 10 elementos de la serie es: %.2f\n", suma);
  return 0;
}
